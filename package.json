{
  "private": true,
  "name": "hack-week-august-2017",
  "version": "0.0.1",
  "repository": "git@github.com:andrew-codes/hack-week-august-2017.git",
  "author": "VersionOne",
  "contributors": [
    "Andrew Smith <andrew.codes>"
  ],
  "license": "MIT",
  "engines": {
    "node": "6.11.2",
    "npm": "latest"
  },
  "scripts": {
    "api/run": "build/bin/start-api.sh",
    "api/restore": "build/bin/restore-api.sh",
    "bootstrap": "concurrently 'build/bin/bootstrap.sh' 'npm run api/restore'",
    "bootstrap/single": "build/bin/bootstrap.single.sh",
    "lint": "build/bin/lint.sh",
    "native": "build/bin/start-native.sh start",
    "native/eject": "build/bin/start-native.sh eject",
    "native/android": "build/bin/start-native.sh android",
    "native/ios": "build/bin/start-native.sh ios",
    "native/start": "concurrently 'npm run native/ios' 'npm run api/run'",
    "prepublish/single": "cross-env NODE_ENV=production ./build/bin/prepublish.single.sh",
    "test": "cross-env NODE_ENV=test build/bin/test.sh",
    "test/single": "cross-env NODE_ENV=test build/bin/test.single.sh",
    "validate": "concurrently 'npm test' 'npm run lint'",
    "web/run": "build/bin/start-web.sh",
    "web/start": "concurrently 'npm run api/run' 'npm run web/run'"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chalk-cli": "^3.0.0",
    "concurrently": "^3.4.0",
    "cross-env": "^5.0.1",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^4.0.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.3.0",
    "file-loader": "^0.11.2",
    "glob": "^7.1.2",
    "jest": "^20.0.4",
    "json-loader": "^0.5.4",
    "lerna": "^2.0.0-rc.5",
    "nodemon": "^1.11.0",
    "react-hot-loader": "next",
    "url-loader": "^0.5.9",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  }
}
